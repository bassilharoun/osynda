<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="paymentMethodOneVM" type="com.osynda.app.modules.paymentmethodone.data.viewmodel.PaymentMethodOneVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearPaymentmethod"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowarrowdown"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageButton
                    android:background="@drawable/rectangle_border_gray_800_radius_16_095"
                    android:id="@+id/btnArrowdown"
                    android:layout_height="@dimen/_32pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_32pxh"
                    android:paddingHorizontal="@dimen/_10pxh"
                    android:paddingVertical="@dimen/_10pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_arrowdown_gray_800_32x32" tools:ignore="ContentDescription"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTicketinformat"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_17pxh"
                    android:layout_weight="0.87"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{paymentMethodOneVM.paymentMethodOneModel.txtTicketinformat}"
                    style="@style/txtPoppinsregular24" tools:text="@string/msg_ticket_informat"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearRowpersonalinfo"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtPersonalinfo"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.81" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{paymentMethodOneVM.paymentMethodOneModel.txtPersonalinfo}"
                style="@style/txtPoppinsregular19" tools:text="@string/lbl_personal_info"/>
            <TextView android:ellipsize="end" android:id="@+id/txtEdit"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_207pxh"
                android:layout_weight="0.19" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{paymentMethodOneVM.paymentMethodOneModel.txtEdit}"
                style="@style/txtPoppinsregular16_6" tools:text="@string/lbl_edit"/>
        </LinearLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtNameMariamN"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{paymentMethodOneVM.paymentMethodOneModel.txtNameMariamN}"
            style="@style/txtPoppinsregular16_4" tools:text="@string/msg_name_mariam_n"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtEmailmariamna"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{paymentMethodOneVM.paymentMethodOneModel.txtEmailmariamna}"
            style="@style/txtPoppinsregular16_4" tools:text="@string/msg_email_mariamna"/>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnrectangleseventysix"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_9pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
            <FrameLayout android:id="@+id/frameStackrectangleseventysix"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_199pxv"
                android:layout_marginEnd="@dimen/_2pxh"
                android:layout_marginStart="@dimen/_2pxh" android:layout_width="@dimen/_364pxh">
                <ImageView android:id="@+id/imageRectangleSeventySix"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_199pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_10pxh}"
                    imageRes="@{@drawable/img_rectangle76_199x364}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle76_199x364"/>
                <LinearLayout android:id="@+id/linearColumn23feb"
                    android:layout_gravity="top|end"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_9pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="wrap_content"
                    android:orientation="vertical" style="@style/groupStyledeep_purple_3">
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txt23Feb"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginEnd="@dimen/_9pxh"
                        android:layout_marginStart="@dimen/_9pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="match_parent"
                        android:text="@{paymentMethodOneVM.paymentMethodOneModel.txt23Feb}"
                        android:textAllCaps="true"
                        style="@style/txtPoppinsmedium14_1" tools:text="@string/lbl_23_feb"/>
                </LinearLayout>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguage"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{paymentMethodOneVM.paymentMethodOneModel.txtLanguage}"
                style="@style/txtPoppinsmedium15_1" tools:text="@string/msg_1_billion_follo"/>
            <LinearLayout
                android:id="@+id/linearRowticketidshtwentythree"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_11pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <LinearLayout android:id="@+id/linearRowellipsenine"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.55"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageEllipseNine"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="centerCrop"
                        circular="@{true}"
                        imageRes="@{@drawable/img_ellipse9_17x17}"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse9_17x17"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtOrganizedbywa"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_weight="0.84"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{paymentMethodOneVM.paymentMethodOneModel.txtOrganizedbywa}"
                        style="@style/txtPoppinsregular9" tools:text="@string/msg_organized_by_wa"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTicketIDSHTwentyThree"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_51pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.45"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{paymentMethodOneVM.paymentMethodOneModel.txtTicketIDSHTwentyThree}"
                    style="@style/txtPoppinsmedium12" tools:text="@string/msg_ticket_id_sh23"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowpaymentmethod"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtPaymentmethod"
                android:layout_height="wrap_content"
                android:layout_weight="0.85" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{paymentMethodOneVM.paymentMethodOneModel.txtPaymentmethod}"
                style="@style/txtPoppinsregular19" tools:text="@string/lbl_payment_method"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtEditOne"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginStart="@dimen/_157pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.15" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{paymentMethodOneVM.paymentMethodOneModel.txtEditOne}"
                style="@style/txtPoppinsregular16_6" tools:text="@string/lbl_edit"/>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowsettings"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_19pxh"
            android:layout_marginStart="@dimen/_19pxh"
            android:layout_marginTop="@dimen/_9pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageSettings"
                android:layout_height="@dimen/_12pxh"
                android:layout_marginBottom="@dimen/_14pxv"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="@dimen/_39pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_settings_12x39"
                tools:ignore="ContentDescription" tools:src="@drawable/img_settings_12x39"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtCardnumberFour"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_weight="0.78" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{paymentMethodOneVM.paymentMethodOneModel.txtCardnumberFour}"
                style="@style/txtPoppinsregular14" tools:text="@string/msg_card_number_4"/>
        </LinearLayout>
        <androidx.recyclerview.widget.RecyclerView
            android:gravity="center_horizontal"
            android:id="@+id/recyclerListticketcounter"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
            itemSpace="@{@dimen/_189pxh}" tools:itemCount="2" tools:listitem="@layout/row_listticketcounter"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnProceedToCheckout"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_42pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_18pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_18pxv" android:singleLine="true"
            android:text="@string/msg_proceed_to_chec"
            android:textAllCaps="false" style="@style/btnSolidRounded_4" tools:text="@string/msg_proceed_to_chec"/>
    </LinearLayout>
</layout>
