<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listrectanglethirteenRowModel" type="com.osynda.app.modules.customerhomeopage.data.model.ListrectanglethirteenRowModel"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnrectanglethirteen"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content" android:orientation="vertical">
        <FrameLayout android:id="@+id/frameStackrectanglethirteen"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_150pxv"
            android:layout_marginEnd="@dimen/_1pxh" android:layout_width="@dimen/_255pxh">

            <ImageView
                android:id="@+id/imageRectangleThirteen"
                cornerRadius="@{@dimen/_6pxh}"
                imageRes="@{@drawable/img_rectangle13}"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_150pxh"
                android:layout_gravity="start"
                android:scaleType="fitXY"
                tools:ignore="ContentDescription"
                tools:src="@drawable/img_rectangle13" />
            <LinearLayout android:id="@+id/linearColumn23feb"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_4pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="wrap_content"
                android:orientation="vertical" style="@style/groupStyledeep_purple_2">
                <TextView android:gravity="center_horizontal"
                    android:id="@+id/txt23FebThree"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginEnd="@dimen/_7pxh"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_width="match_parent"
                    android:text="@{listrectanglethirteenRowModel.txt23FebThree}"
                    android:textAllCaps="true"
                    style="@style/txtPoppinsmedium11" tools:text="@string/lbl_23_feb"/>
            </LinearLayout>
        </FrameLayout>

        <LinearLayout
            android:id="@+id/linearRowprice"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_6pxv"
            android:layout_marginEnd="@dimen/_7pxh"
            android:gravity="start"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txtPriceThree"
                style="@style/txtPoppinssemibold10"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.84"
                android:text="@{listrectanglethirteenRowModel.txtPriceThree}"
                android:textSize="@dimen/_12pxh"
                tools:text="@string/msg_yayoi_kusama_i" />

            <TextView
                android:id="@+id/txtFrame355Three"
                style="@style/txtSolidRoundedOutline_1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginBottom="@dimen/_18pxv"
                android:layout_weight="0.16"
                android:ellipsize="end"
                android:paddingStart="@dimen/_5pxh"
                android:paddingTop="@dimen/_1pxv"
                android:paddingEnd="@dimen/_7pxh"
                android:paddingBottom="@dimen/_1pxv"
                android:singleLine="true"
                android:text="@{listrectanglethirteenRowModel.txtFrame355Three}"
                tools:text="@string/lbl_art2" />
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowtime"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_3pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguageTwo"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.52" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{listrectanglethirteenRowModel.txtLanguageTwo}"
                style="@style/txtPoppinslight6" tools:text="@string/msg_location_nasr"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowellipsenine"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_width="wrap_content" android:orientation="horizontal">
            <ImageView android:id="@+id/imageEllipseNine"
                android:layout_height="@dimen/_17pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_17pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_ellipse9}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse9"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtOrganizedbyYA"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_weight="0.63" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{listrectanglethirteenRowModel.txtOrganizedbyYA}"
                style="@style/txtPoppinsregular7_1" tools:text="@string/msg_organized_by_ya"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPriceOne"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_23pxh"
                android:layout_weight="0.26" android:layout_width="0dp"
                android:paddingBottom="@dimen/_2pxv"
                android:paddingEnd="@dimen/_6pxh"
                android:paddingStart="@dimen/_5pxh"
                android:paddingTop="@dimen/_3pxv"
                android:singleLine="true"
                android:text="@{listrectanglethirteenRowModel.txtPriceOne}"
                style="@style/txtSolidRounded_1" tools:text="@string/lbl_35_00"/>
        </LinearLayout>
    </LinearLayout>
</layout>
