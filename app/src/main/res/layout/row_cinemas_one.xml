<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="cinemasOneRowModel" type="com.osynda.app.modules.cinemasone.data.model.CinemasOneRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowrectanglesixtysix"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageRectangleSixtySix"
            android:layout_height="@dimen/_114pxh"
            android:layout_width="@dimen/_100pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_6pxh}"
            imageRes="@{@drawable/img_rectangle66}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle66"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnframe355"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginStart="@dimen/_12pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_weight="0.71" android:layout_width="0dp" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtFrame355"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="wrap_content"
                android:paddingBottom="@dimen/_2pxv"
                android:paddingEnd="@dimen/_8pxh"
                android:paddingStart="@dimen/_8pxh"
                android:paddingTop="@dimen/_2pxv"
                android:singleLine="true"
                android:text="@{cinemasOneRowModel.txtFrame355}"
                style="@style/txtSolidRoundedOutline_3" tools:text="@string/lbl_gaming3"/>
            <LinearLayout android:id="@+id/linearRowlanguage"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.72"
                    android:layout_width="0dp"
                    android:text="@{cinemasOneRowModel.txtLanguage}"
                    style="@style/txtPoppinsmedium13" tools:text="@string/msg_cyberpunk_2077"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btn35Zero"
                    android:layout_height="@dimen/_26pxv"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginStart="@dimen/_48pxh"
                    android:layout_width="@dimen/_56pxh"
                    android:paddingBottom="@dimen/_3pxv"
                    android:paddingEnd="@dimen/_8pxh"
                    android:paddingStart="@dimen/_8pxh"
                    android:paddingTop="@dimen/_3pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_35_00"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_35_00"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowlocationicnop"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageLocationicnop"
                    android:layout_height="@dimen/_8pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_7pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_locationicnop"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_locationicnop"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNasrcitycairo"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.26"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{cinemasOneRowModel.txtNasrcitycairo}"
                    style="@style/txtPoppinslight8" tools:text="@string/lbl_nasr_city_cairo"/>
                <ImageView android:id="@+id/imageClock"
                    android:layout_height="@dimen/_9pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_11pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_9pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_clock_black_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_clock_black_900"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtOctober14202"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.67"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{cinemasOneRowModel.txtOctober14202}"
                    style="@style/txtPoppinsregular8" tools:text="@string/msg_october_14_202"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowtelevision"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageTelevision"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_width="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_television"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_television"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtOffer"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.95"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{cinemasOneRowModel.txtOffer}"
                    style="@style/txtMontserratromanregular11" tools:text="@string/msg_get_discount_34"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
