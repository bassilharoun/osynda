<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="customerHomeOpageContainerVM" type="com.osynda.app.modules.customerhomeopagecontainer.data.viewmodel.CustomerHomeOpageContainerVM"/>
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout android:gravity="start"
        android:id="@+id/linearRowrectanglefive"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">

        <androidx.fragment.app.FragmentContainerView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/fragmentContainer" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:background="@color/white"
            app:layout_constraintStart_toStartOf="parent">

            <LinearLayout
                android:id="@+id/linearRowhome"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/_14pxv"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/linearHome"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageHome"
                        android:layout_width="@dimen/_19pxh"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_gravity="center"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_home"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_home" />

                    <TextView
                        android:id="@+id/txtHomeOne"
                        style="@style/txtPoppinsregular12_2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:singleLine="true"
                        android:text="@{customerHomeOpageContainerVM.customerHomeOpageContainerModel.txtHomeOne}"
                        tools:text="@string/lbl_home" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearDicover"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageRewind"
                        android:layout_width="@dimen/_18pxh"
                        android:layout_height="@dimen/_19pxh"
                        android:layout_gravity="center"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_rewind"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_rewind" />

                    <TextView
                        android:id="@+id/txtSearch"
                        style="@style/txtPoppinsregular12_3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:singleLine="true"
                        android:text="@{customerHomeOpageContainerVM.customerHomeOpageContainerModel.txtSearch}"
                        tools:text="@string/lbl_search" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearTickets"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageThumbsup"
                        android:layout_width="@dimen/_20pxh"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_gravity="center"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_thumbsup"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_thumbsup" />

                    <TextView
                        android:id="@+id/txtTickets"
                        style="@style/txtPoppinsregular12_3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:singleLine="true"
                        android:text="@{customerHomeOpageContainerVM.customerHomeOpageContainerModel.txtTickets}"
                        tools:text="@string/lbl_tickets2" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearProfile"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageSearchOne"
                        android:layout_width="@dimen/_14pxh"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_gravity="center"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_search_gray_600"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_search_gray_600" />

                    <TextView
                        android:id="@+id/txtProfile"
                        style="@style/txtPoppinsregular12_3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_marginEnd="@dimen/_2pxh"
                        android:ellipsize="end"
                        android:gravity="center_horizontal"
                        android:singleLine="true"
                        android:text="@{customerHomeOpageContainerVM.customerHomeOpageContainerModel.txtProfile}"
                        tools:text="@string/lbl_profile" />
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
